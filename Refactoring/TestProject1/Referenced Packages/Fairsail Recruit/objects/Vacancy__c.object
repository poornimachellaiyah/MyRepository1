        <formula>if(  Hide_Salary__c==True, &apos;&apos;, 

if( isblank( Salary_Minimum__c) || Salary_Minimum__c==Salary_Maximum__c  , &apos;&apos;,  

Currency_Code__c&amp;&apos; &apos; &amp;
IF(
  Salary_Minimum__c &gt;= 1000000000,
  TEXT(FLOOR(Salary_Minimum__c / 1000000000)) &amp; &quot;,&quot;,
  &quot;&quot;) &amp;
IF(
  Salary_Minimum__c &gt;= 1000000,
  TEXT(FLOOR(Salary_Minimum__c / 1000000)) &amp; &quot;,&quot;,
  &quot;&quot;) &amp;
IF(
  Salary_Minimum__c &gt;= 1000,
  RIGHT(TEXT(FLOOR(Salary_Minimum__c / 1000)), 3) &amp; &quot;,&quot;,
  &quot;&quot;) &amp; 
  RIGHT(TEXT(FLOOR(Salary_Minimum__c)), 3)
&amp; &apos; - &apos;  )

+  if( isblank(Salary_Maximum__c) , &apos;&apos;,  

Currency_Code__c&amp;&apos; &apos; &amp;
IF(
  Salary_Maximum__c &gt;= 1000000000,
  TEXT(FLOOR(Salary_Maximum__c / 1000000000)) &amp; &quot;,&quot;,
  &quot;&quot;) &amp;
IF(
  Salary_Maximum__c &gt;= 1000000,
  TEXT(FLOOR(Salary_Maximum__c / 1000000)) &amp; &quot;,&quot;,
  &quot;&quot;) &amp;
IF(
  Salary_Maximum__c &gt;= 1000,
  RIGHT(TEXT(FLOOR(Salary_Maximum__c / 1000)), 3) &amp; &quot;,&quot;,
  &quot;&quot;) &amp; 
  RIGHT(TEXT(FLOOR(Salary_Maximum__c)), 3)

))</formula>
